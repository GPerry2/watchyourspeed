  "dashboard": [
    {
      "title": "Total Submissions",
      "type": "header",
      "id": "fst",
      "li": [
        {
          "title": "Today",
          "type": "list_item",
          "id": "fst_today",
          "filter":"__CreatedOn ge {{today_d}} and __CreatedOn lt {{today_end}}",
          "href": "#/submissions?status=Today's&filter=__CreatedOn*__CreatedOn ge {{today_d}} and __CreatedOn lt {{today_end}}",
          "entity": "submissions"
        },
        {
          "title": "This Week",
          "type": "list_item",
          "id": "fst_week",
          "filter":"__CreatedOn ge {{last_week}} and __CreatedOn lt {{today_end}}",
          "href": "#/submissions?status=Last Week&filter=__CreatedOn*__CreatedOn ge {{last_week}} and __CreatedOn lt {{today_end}}",
          "entity": "submissions"
        },
        {
          "title": "This Month",
          "type": "list_item",
          "id": "fst_month",
          "filter":"__CreatedOn ge {{last_month}} and __CreatedOn lt {{today_end}}",
          "href": "#/submissions?status=Last Month&filter=__CreatedOn*__CreatedOn ge {{last_month}} and __CreatedOn lt {{today_end}}",
          "entity": "submissions"
        },
        {
          "title": "Older",
          "type": "list_item",
          "id": "fst_older",
          "filter":"__CreatedOn le {{last_month}}",
          "href": "#/submissions?status=Older&filter=__CreatedOn*__CreatedOn le {{last_month}}",
          "entity": "submissions"
        }

      ]
    }
  ]
